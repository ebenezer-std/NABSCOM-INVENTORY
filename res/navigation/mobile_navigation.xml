<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.mobile.nabbscom.ProductHomeFragment"
        android:label="@string/app_name"
        tools:layout="@layout/producthome" />

    <fragment
        android:id="@+id/navigation_categories"
        android:name="com.mobile.nabbscom.ProductCategoryFragment"
        android:label="Category"
        tools:layout="@layout/productcategory" />

    <fragment
        android:id="@+id/navigation_customer"
        android:name="com.mobile.nabbscom.CustomerCareFragment"
        android:label="@string/menu_customer"
        tools:layout="@layout/customercare" />

    <fragment
        android:id="@+id/navigation_authenticate"
        android:name="com.mobile.nabbscom.AuthenticateFragment"
        android:label="Account"
        tools:layout="@layout/account_authenticate" />
</navigation>